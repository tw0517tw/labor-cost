{
  "private": true,
  "name": "labor-cost",
  "homepage": "http://tw0517tw.github.io/labor-cost",
  "version": "0.1.1",
  "scripts": {
    "build": "react-scripts build",
    "deploy": "npm run build && gh-pages -d build",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "prepare": "husky install",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.4",
    "reactstrap": "^6.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.6.1",
    "eslint-config-yoctol": "^0.18.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.3.0",
    "gh-pages": "^2.0.1",
    "husky": "^7.0.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "prettier-package-json": "^2.0.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.babelrc": [
      "prettier --parser json --write",
      "git add"
    ],
    "*package.json": [
      "prettier-package-json --write",
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true
  }
}
